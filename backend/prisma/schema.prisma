
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url = env("DATABASE_URL")
}

model Tweet {
  id          Int      @id @default(autoincrement())
  tweetId     String   @unique
  text        String?
  authorId    String?
  createdAt   DateTime?
  raw         Json?
  importedAt  DateTime @default(now())
}